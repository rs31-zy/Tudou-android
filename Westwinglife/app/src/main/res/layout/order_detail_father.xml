<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="#ffffff"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout android:id="@+id/llt_goods"
        android:layout_marginTop="3dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </LinearLayout>
<LinearLayout android:orientation="vertical"
    android:layout_width="match_parent"
    android:gravity="center_vertical"
    android:layout_height="wrap_content">
    <LinearLayout android:orientation="horizontal"
        android:layout_marginLeft="220dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView android:text="本仓商品总价:"
            android:textSize="12sp"
            android:textColor="@color/gray_six"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/tv_self_goods_price"
            android:text="--"
            android:textSize="12sp"
            android:textColor="@color/gray_six"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout android:orientation="horizontal"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="220dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView android:text="本仓税费:"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="12sp"
            android:textColor="@color/gray_six"
            android:layout_height="wrap_content" />
        <TextView android:text="--"
            android:textSize="12sp"
            android:id="@+id/tv_self_custom"
            android:textColor="@color/gray_six"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout android:orientation="horizontal"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="220dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView android:text="本仓运费:"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="12sp"
            android:textColor="@color/gray_six"
            android:layout_height="wrap_content" />
        <TextView android:text="--"
            android:textSize="12sp"
            android:id="@+id/tv_ship_ment_fee"
            android:textColor="@color/gray_six"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout android:orientation="horizontal"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="220dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView android:text="本仓合计:"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textColor="@color/black_base"
            android:layout_height="wrap_content" />
        <TextView android:text="--"
            android:id="@+id/tv_self_all_price"
            android:textColor="@color/color_selected"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
</LinearLayout>

</LinearLayout>